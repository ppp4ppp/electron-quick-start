{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --arm64"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub <test@test234322344.com>",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^16.0.7",
    "electron-builder": "^22.14.13"
  },
  "dependencies": {
    "yarn": "^1.22.17"
  },
  "build": {
    "appId": "your.id",
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "arm64"
          ]
        }
      ]
    }
  }
}
